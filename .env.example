# Backend
JWT_SECRET=your-secret-key-change-in-production
# Master encryption key for encrypting user data keys (32 bytes base64-encoded)
# Generate with: python -c "import os, base64; print(base64.b64encode(os.urandom(32)).decode())"
MASTER_KEY=your_master_key_here

# Database
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=se_detector

# Annotation Service - Multiple LLM API Keys for load balancing

# Groq API (FASTEST! 30 req/min, 14,400 req/day) - Get key from https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# OpenRouter API (FREE tier available!) - Get key from https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Gemini API Keys - Get from https://aistudio.google.com/app/apikey (use different Google accounts)
GEMINI_API_KEY=your_gemini_api_key_here
# Optional: Add second Gemini API key below (leave empty if not needed)
GEMINI_API_KEY_2=
# Optional: Add third Gemini API key below (leave empty if not needed)
GEMINI_API_KEY_3=

# Telegram Collector Configuration
# Get API ID and Hash from https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash
TELEGRAM_PHONE=+your_phone_number

# VK Collector Configuration (optional)
# Get App ID from https://vk.com/apps?act=manage
VK_APP_ID=0
VK_ACCESS_TOKEN=
VK_ENABLED=false
